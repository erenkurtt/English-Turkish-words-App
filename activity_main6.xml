<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgg"
    tools:context=".MainActivity6">

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="119dp"
        android:layout_height="84dp"
        android:layout_marginTop="40dp"
        android:adjustViewBounds="true"

        android:contentDescription="@string/todo"
        android:padding="20dp"
        android:scaleType="fitCenter"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:srcCompat="@android:drawable/stat_notify_sync" />

    <TextView
        android:id="@+id/Question"
        android:layout_width="match_parent"
        android:layout_height="136dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/paytone_one"
        android:gravity="center"
        android:text="@string/Question"
        android:textSize="40sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/success"
        android:layout_width="match_parent"
        android:layout_height="190dp"
        android:layout_marginTop="16dp"
        android:fontFamily="@font/paytone_one"
        android:gravity="center"
        android:text="@string/success"
        android:textSize="40sp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button7"
        android:layout_width="347dp"
        android:layout_height="73dp"
        android:layout_marginTop="20dp"
        android:background="#792B2424"
        android:text="@string/button7"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Question" />

    <Button
        android:id="@+id/button8"
        android:layout_width="347dp"
        android:layout_height="73dp"
        android:layout_marginTop="20dp"
        android:background="#792B2424"
        android:text="@string/button8"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button7" />

    <Button
        android:id="@+id/button9"
        android:layout_width="347dp"
        android:layout_height="73dp"
        android:layout_marginTop="20dp"
        android:background="#792B2424"
        android:text="@string/button9"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button8" />

    <Button
        android:id="@+id/button10"
        android:layout_width="254dp"
        android:layout_height="88dp"
        android:background="#C8081235"
        android:drawableStart="@drawable/vid"
        android:drawableLeft="@drawable/vid"
        android:padding="20dp"
        android:text="@string/button10"
        android:textSize="24sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textFailed"
        app:layout_constraintVertical_bias="0.178" />

    <Button
        android:id="@+id/button11"
        android:layout_width="254dp"
        android:layout_height="88dp"
        android:background="#374350"
        android:drawableStart="@drawable/backicon"
        android:drawableLeft="@drawable/backicon"
        android:padding="20dp"
        android:text="@string/button11"
        android:textSize="24sp"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button10"
        app:layout_constraintVertical_bias="0.141" />

    <TextView
        android:id="@+id/textFailed"
        android:layout_width="match_parent"
        android:layout_height="144dp"
        android:layout_marginTop="112dp"
        android:fontFamily="@font/paytone_one"
        android:gravity="center"
        android:text="@string/textFailed"
        android:textSize="28sp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Question" />

    <TextView
        android:id="@+id/textFailed2"
        android:layout_width="match_parent"
        android:layout_height="144dp"
        android:layout_marginTop="112dp"
        android:fontFamily="@font/paytone_one"
        android:gravity="center"
        android:text="@string/textFailed2"
        android:textSize="30sp"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Question" />

    <TextView
        android:id="@+id/counterQuest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:gravity="center"
        android:text="@string/counterQuest"
        android:textSize="33sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button9" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="match_parent"
        android:layout_height="215dp"
        android:layout_marginTop="16dp"
        android:contentDescription="@string/todo2"
        android:visibility="invisible"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/success"
        app:srcCompat="@drawable/check" />

</androidx.constraintlayout.widget.ConstraintLayout>